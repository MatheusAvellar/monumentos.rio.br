	<header>
		<nav class="downsize">
			<ul>
				<li><a href="/">monumentos.rio</a></li>
				<li>
					<details>
						<summary>Inventário</summary>
						<ul>
							<li>
								<a href="/todos">
									<svg class="icon"><use href="#all"/></svg>
									<span>Todos</span>
								</a>
							</li>
							<li class="<%= catToClass("obra") %>">
								<a href="/obras">
									<svg class="icon"><use href="#public"/></svg>
									<span><%= catToName("obra") %></span>
								</a>
							</li>
							<li class="<%= catToClass("busto") %>">
								<a href="/bustos">
									<svg class="icon"><use href="#bust"/></svg>
									<span><%= catToName("busto") %></span>
								</a>
							</li>
							<li class="<%= catToClass("chafariz") %>">
								<a href="/chafarizes">
									<svg class="icon"><use href="#fountain"/></svg>
									<span><%= catToName("chafariz") %></span>
								</a>
							</li>
							<li class="<%= catToClass("estátua") %>">
								<a href="/estatuas">
									<svg class="icon"><use href="#statue"/></svg>
									<span><%= catToName("estátua") %></span>
								</a>
							</li>
							<li class="<%= catToClass("marco") %>">
								<a href="/marcos">
									<svg class="icon"><use href="#mark"/></svg>
									<span><%= catToName("marco") %></span>
								</a>
							</li>
							<li class="<%= catToClass("escultura") %>">
								<a href="/esculturas">
									<svg class="icon"><use href="#sculpture"/></svg>
									<span><%= catToName("escultura") %></span>
								</a>
							</li>
						</ul>
					</details>
				</li>
				<li>
					<a href="/mapa">
						<span>Mapa</span>
					</a>
				</li>
				<li>
					<details>
						<summary>Outros</summary>
						<ul>
							<li class="menu-description">
								<span class="menu-description">Organizar monumentos por...</span>
								<hr>
							</li>
							<li>
								<a href="/bairros">
									<span>Bairro</span>
								</a>
							</li>
							<li>
								<a href="/autores">
									<span>Autor da obra</span>
								</a>
							</li>
						</ul>
					</details>
				</li>
				<li><a href="/sobre">Sobre</a></li>
				<li><a href="/mais">Mais</a></li>
				<li><a href="/creditos">Créditos</a></li>
			</ul>
		</nav>
	</header>
	<%- content %>
